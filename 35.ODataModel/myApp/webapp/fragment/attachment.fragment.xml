<more:FragmentDefinition xmlns:more="sap.ui.core" xmlns="sap.m">
  <Table id="idTable" mode="SingleSelectMaster" items="{/ProductSet}" itemPress="onItemPressSupp" >
    <headerToolbar>
       <Toolbar >
         <ToolbarSpacer />  
         <Button icon="sap-icon://filter" tooltip = "Filter Data" press = "onFilter"></Button>
         <Button icon="sap-icon://clear-filter" tooltip="Remove Filters" press ="onRemoveFilter"/>
       </Toolbar>
    </headerToolbar>
                <columns>
                  <Column >
                    <header>
                      <Label text="Name of Product"></Label>
                    </header>
                  </Column>
                  <Column >
                    <header>
                      <Label text="Currency Code"></Label>
                    </header>
                  </Column>
                  <Column >
                    <header>
                      <Label text="Product Id"></Label>
                    </header>
                  </Column>
                  <Column minScreenWidth="Tablet">
                    <header>
                      <Label text="Price"></Label>
                    </header>
                  </Column>
                </columns>
                <items>
                  <ColumnListItem type="Navigation">
                    <cells>
                      <Text text="{NAME}"></Text>
                    </cells>
                    <cells>
                      <Input value="{CURRENCY_CODE}" showValueHelp="true" valueHelpRequest="onF4help"></Input>
                    </cells>
                    <cells>
                      <Text text="{PRODUCT_ID}"></Text>
                    </cells>
                    <cells>
                      <Text text="{PRICE}"></Text>
                    </cells>
                  </ColumnListItem>
                </items>
             
             </Table>
</more:FragmentDefinition>